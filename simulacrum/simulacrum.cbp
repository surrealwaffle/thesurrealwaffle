				<ExtraCommands>
					<Add after='[[ IO.CopyFile(ReplaceMacros(_(&quot;$(WORKSPACE_DIR)\\simulacrum\\bin\\Release\\simulacrum.dll&quot;)), ReplaceMacros(_(&quot;$(WORKSPACE_DIR)\\build\\modules\\simulacrum.dll&quot;)), true); ]]' />
